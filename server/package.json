{
  "name": "mindworkflow-server",
  "version": "0.6.7",
  "private": true,
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p . && cp -r src/schemas dist/",
    "start": "npx ts-node src/index.ts",
    "test": "jest --config ../jest.config.ts --runInBand",
    "pkg": "pkg dist/index.js --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --out-path ../releases",
    "package": "npm run build && npm run pkg"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "adm-zip": "^0.5.10",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.7",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^9.15.1",
    "googleapis": "^140.0.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "pino": "^10.3.1",
    "puppeteer": "^23.6.1",
    "replicate": "^1.3.0",
    "uuid": "^9.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/knex": "^0.16.1",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^9.0.7",
    "pkg": "^5.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "pkg": {
    "assets": [
      "../app/dist/**/*",
      "src/schemas/**/*",
      "node_modules/better-sqlite3/build/Release/better_sqlite3.node"
    ],
    "scripts": "dist/**/*.js"
  }
}
